<Window xmlns:awe="http://schemas.awesomium.com/winfx"  x:Class="PageKeeper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PageKeeper"
        mc:Ignorable="d"
        Name="winMain"
        Title="PageKeeper" 
        Height="350" Width="500" 
        Closing="window_Closing">
    <DockPanel Name="stackPanel" LastChildFill="True">
        <TextBox x:Name="txtUrl" 
                 Height="25" Background="Black" Foreground="#FFE8E8E8" 
                 DockPanel.Dock="Top"
                 KeyUp="textBox_KeyUp"/>
        <awe:WebControl Name="webMain"/>
    </DockPanel>
</Window>
